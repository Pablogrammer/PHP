{% extends 'base.html.twig' %}

{% block title %}Prestamo{% endblock %}

{% block body %}
    <h1>Prestamo</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prestamo.id }}</td>
            </tr>
            <tr>
                <th>Fecha_inicio</th>
                <td>{{ prestamo.fechaInicio ? prestamo.fechaInicio|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Fecha_fin</th>
                <td>{{ prestamo.fechaFin ? prestamo.fechaFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_prestamo_index') }}">back to list</a>

    <a href="{{ path('app_prestamo_edit', {'id': prestamo.id}) }}">edit</a>

    {{ include('prestamo/_delete_form.html.twig') }}
{% endblock %}
